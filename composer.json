{
  "name": "njimx/nova-notes-field",
  "description": "This Laravel Nova package adds a notes field to Nova's arsenal of fields.",
  "keywords": [
    "laravel",
    "nova",
    "optimistdigital",
    "field"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "optimistdigital/nova-translations-loader": "^5.0"
  },
  "require-dev": {
    "laravel/nova": "^4.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "OptimistDigital\\NovaNotesField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\NovaNotesField\\NotesFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
